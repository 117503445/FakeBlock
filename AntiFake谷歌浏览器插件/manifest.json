{
  "manifest_version": 2,

  "name": "AntiFake",
  "description": "信息安全竞赛作品，检测AI生成的虚假图片和文章",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "antifake.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "https://*/*"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self';"
}